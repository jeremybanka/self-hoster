[core]
  pager = diff-so-fancy | less --tabs=4 -RFX
[rebase]
  autosquash = true
[alias]
  a = add
  aa = add --all
  b = branch
  bn = checkout -b
  c = commit
  ca = commit --amend
  ch = checkout
  cm = commit --message
  d = diff
  dn = pull
  l = log --oneline
  la = log --oneline --abbrev-commit --all --graph --decorate --color
  ll = log
  on = push --set-upstream
  onhi = push --set-upstream origin main
  r = reset
  ra = reset .
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase --interactive
  rf = reflog
  rmc = rm -r --cached .
  rv = remote --verbose
  s = status
  sh = stash
  up = push
[color]
  ui = true
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold reverse
  newNormal = green bold
  newHighlight = green bold reverse
[color "diff"]
  meta = 11
  frag = magenta bold
  func = 146 bold
  commit = yellow bold
  old = red bold
  new = green bold
  whitespace = red reverse
